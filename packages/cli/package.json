{
  "name": "@semo/cli",
  "version": "0.0.11",
  "description": "A command line tools dispatcher ",
  "keywords": [
    "cli",
    "command"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vipzhicheng/semo"
  },
  "license": "MIT",
  "bin": {
    "semo": "lib/main.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "chalk": "^4.0.0",
    "dayjs": "^1.8.25",
    "debug": "^4.1.1",
    "dumper.js": "^1.3.1",
    "envinfo": "^7.5.0",
    "find-up": "^4.1.0",
    "fs-extra": "^9.0.0",
    "fuzzy": "^0.1.3",
    "get-stdin": "^7.0.0",
    "glob": "^7.1.6",
    "inquirer": "^7.1.0",
    "json-colorizer": "^2.2.1",
    "json-stringify-pretty-compact": "^2.0.0",
    "lodash": "^4.17.15",
    "node-cache": "^5.1.0",
    "node-emoji": "^1.10.0",
    "node-object-hash": "^2.0.0",
    "shelljs": "^0.8.3",
    "table": "^5.4.6",
    "update-notifier": "^4.1.0",
    "yargs": "^15.3.1",
    "yargs-parser": "^18.1.3"
  },
  "devDependencies": {
    "@types/co": "^4.6.2",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^8.1.0",
    "@types/get-stdin": "^7.0.0",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.2",
    "@types/node-cache": "^4.2.5",
    "@types/node-emoji": "^1.8.1",
    "@types/shelljs": "^0.8.7",
    "@types/table": "^5.0.0",
    "@types/update-notifier": "^4.1.0",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "vuepress": "^1.4.1"
  },
  "scripts": {
    "semo": "node lib/main.js",
    "watch": "tsc -w",
    "build": "tsc",
    "clean": "rimraf lib",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "docs:typedoc": "mkdir -p doc/.vuepress/public/typedoc && typedoc src/common",
    "docs:dev": "npm run docs:typedoc && vuepress dev doc",
    "docs:build": "npm run docs:typedoc && vuepress build doc"
  },
  "rc": {
    "commandDir": "lib/commands",
    "commandMakeDir": "src/commands",
    "hookDir": "lib/hooks",
    "pluginDir": "lib/plugins",
    "pluginMakeDir": "src/plugins"
  },
  "engines": {
    "node": ">=8.10.0"
  }
}

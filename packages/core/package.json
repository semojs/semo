{
  "name": "@semo/core",
  "version": "2.0.21",
  "description": "Semo core",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./utils": {
      "types": "./lib/common/utils.d.ts",
      "import": "./lib/common/utils.js"
    },
    "./log": {
      "types": "./lib/common/log.d.ts",
      "import": "./lib/common/log.js"
    },
    "./hook": {
      "types": "./lib/common/hook.d.ts",
      "import": "./lib/common/hook.js"
    },
    "./prompts": {
      "types": "./lib/common/prompts.d.ts",
      "import": "./lib/common/prompts.js"
    },
    "./debug": {
      "types": "./lib/common/debug.d.ts",
      "import": "./lib/common/debug.js"
    },
    "./template": {
      "types": "./lib/common/template.d.ts",
      "import": "./lib/common/template.js"
    }
  },
  "scripts": {
    "clean": "rimraf lib *.tsbuildinfo",
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "typecheck": "tsc --noEmit",
    "docs:typedoc": "typedoc src/common/*",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "^20.19.0 || ^22.12.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.0",
    "colorize-template": "^1.0.0",
    "debug": "^4.4.0",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "find-up": "^7.0.0",
    "glob": "^11.0.1",
    "json-colorizer": "^3.0.1",
    "json-stringify-pretty-compact": "^4.0.0",
    "picocolors": "^1.1.1",
    "table": "^6.9.0",
    "handlebars": "^4.7.8",
    "yaml": "^2.7.1",
    "yargs": "^18.0.0",
    "yargs-parser": "^22.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "typedoc": "^0.28.3"
  }
}

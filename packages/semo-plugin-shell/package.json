{
  "name": "semo-plugin-shell",
  "version": "2.0.18",
  "description": "A semo plugin to provide a quick shell.",
  "keywords": [
    "cli",
    "command",
    "semo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/semojs/semo"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/shelljs": "^0.8.15",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "clean": "rimraf lib *.tsbuildinfo",
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@semo/core": "workspace:^",
    "fs-extra": "^11.3.0",
    "shelljs": "^0.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}

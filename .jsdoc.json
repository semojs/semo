{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./README.md", "src/common/utils.js"],
    "includePattern": ".js$"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./docs/api",
    "encoding": "utf8",
    "private": true,
    "recurse": false,
    "template": "./node_modules/docdash"
  }
}
